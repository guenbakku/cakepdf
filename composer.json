{
    "name": "guenbakku/cakepdf",
    "description": "CakePHP plugin for converting HTML to PDF",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "NVB",
            "homepage": "http://nvb-online.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "knplabs/knp-snappy": "^0.5.0",
        "h4cc/wkhtmltopdf-i386": "^0.12.3",
        "h4cc/wkhtmltopdf-amd64": "^0.12.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Guenbakku\\Cakepdf\\": "src"
        }
    },
    "config": {
		"vendor-dir": "vendors/"
	}
}
